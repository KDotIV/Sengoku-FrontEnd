{
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html",
      "exclude": [
        "/*.css",
        "/*.js",
        "/*.ico",
        "/*.png",
        "/*.jpg",
        "/*.jpeg",
        "/*.gif",
        "/*.svg",
        "/*.woff",
        "/*.woff2",
        "/*.ttf",
        "/*.eot",
        "/*.wasm"
      ]
    }
  ],
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css",
    ".wasm": "application/wasm"
  }
}
